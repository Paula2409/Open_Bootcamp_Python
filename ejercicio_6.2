class Alumno():
    def __init__(self, nombre, nota):
        self.nombre = nombre
        self.nota = nota
        
    def resultado(self):
        if self.nota >= 4:
            print("El alumno",self.nombre,"aprobo")
        else:
            print("El alumno",self.nombre,"debe recuperar el examen")

alumno = Alumno("Mario Gonzalez",6)
alumno.resultado()
