import math

def calcular_area_triangulo(base,altura):
     return base * altura
    

def calcular_area_circulo(radio):
    return  math.pi * (radio**2)
    
    
area_tr = calcular_area_triangulo(6,8)

area_ci = calcular_area_circulo(5)


print("El area del triangulo es: ", area_tr)
print("El area del circulo es: ", area_ci)
